    "@tailwindcss/vite": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.0.0",
    "vite": "^5.0.8"
  },
